<div class="tab-pane fade" id="panel-search" role="tabpanel">
  <h3>Search</h3>
  <div class="show-hide" id="search-result">
	<p>Key: <span id="search-keyword">AAPL</span></p>
	<table class="table table-hover mt-5 show-hide">
	  <thead>
		  <tr>
			  <th>Symbol</th>
			  <th>Price $</th>
			  <th>Action</th>
		  </tr>
	  </thead>
	  <tbody></tbody>
  </table>
  <div id="result-chart" class="col-8 show-hide mb-5"></div>
  </div>
  <form class="mt-5">
	  <div class="form-row d-flex align-items-end">
		  <div class="col-4">
			  <label for="price" class="my-font-small">Enter Symbol</label>
			  <input id="search-input" type="text" class="form-control" name="price" placeholder="eg. GOOG">
		  </div>
		  <div class="col-12 mt-5">
			  <a id="search-btn" class="btn btn-indigo" href="#0" style="margin:0">Search</a>
		  </div>
	  </div>
  </form>
</div>